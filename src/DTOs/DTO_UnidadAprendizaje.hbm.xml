<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DTOs.DTO_UnidadAprendizaje" table="unidadAprendizaje" schema="dbo" catalog="ODA_local">
        <id name="idUa">
            <column name="id_UA" sql-type="int"/>
        </id>
        <property name="nomUa">
            <column name="nom_UA" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>