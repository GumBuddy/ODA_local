<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DTOs.DTO_TipoObjeto" table="tipoObjeto" schema="dbo" catalog="ODA_local">
        <id name="idTipo">
            <column name="id_tipo" sql-type="int"/>
        </id>
        <property name="tipoObjeto">
            <column name="tipo_objeto" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>